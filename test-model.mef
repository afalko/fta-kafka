<?xml version="1.0" encoding="UTF-8"?>
<opsa-mef>
  <define-fault-tree name="KafkaUptime">
    <define-gate name="ReadWrite">
      <or>
        <event name="Network"/>
        <event name="Broker"/>
      </or>
    </define-gate>
	<define-gate name="Broker">
	  <or>
	    <event name="Zookeeper"/>
		<event name="Disk"/>
		<event name="System"/>
		<event name="OS"/>
	  </or>
    </define-gate>
	<define-gate name="Zookeeper">
      <and>
        <event name="ZK1"/>
        <event name="ZK2"/>
		<event name="ZK3"/>
      </and>
    </define-gate>
	
  </define-fault-tree>
  <model-data>
    <define-basic-event name="Produce">
      <label>Produce/Consume Message Failure</label>
      <float value="1"/>
    </define-basic-event>
    <define-basic-event name="ZK1">
      <float value="0.01"/>
    </define-basic-event>
    <define-basic-event name="Network">
      <float value="0.2"/>
    </define-basic-event>
    <define-basic-event name="ZK2">
      <float value="0.01"/>
    </define-basic-event>
    <define-basic-event name="ZK3">
      <float value="0.01"/>
    </define-basic-event>
	<define-basic-event name="Disk">
      <float value="0.04"/>
    </define-basic-event>
	<define-basic-event name="System">
      <float value="0.01"/>
    </define-basic-event>
	<define-basic-event name="OS">
      <float value="0.10"/>
    </define-basic-event>
  </model-data>
</opsa-mef>
